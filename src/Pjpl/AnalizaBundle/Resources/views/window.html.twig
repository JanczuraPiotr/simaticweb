<!doctype html>
<html lang="{{app.request.locale}}">

	<head>
		<title>{% block title %} Simatic Web | Analiza {% endblock %}</title>
		<meta charset="UTF-8" />

		{% block css %}
			<link href="{{ asset('bundles/pjplanaliza/css/pjpl.css') }}" type="text/css" rel="stylesheet"/>
			<link href="{{ asset('bundles/pjplanaliza/css/scada.css') }}" type="text/css" rel="stylesheet"/>
			<link href="{{ asset('bundles/pjplanaliza/css/menu.css') }}" type="text/css" rel="stylesheet"/>
		{% endblock %}

	</head>

	<body>
		<div class="nav-top">

			{% block menu %}
					{{ knp_menu_render('PjplAnalizaBundle:Builder:AnalizaMenu') }}
			{% endblock %}

		</div>
		{% block body %}{% endblock %}
		{% block templates %}{% endblock %}
		{% block javascripts %}{% endblock %}

	</body>

</html>